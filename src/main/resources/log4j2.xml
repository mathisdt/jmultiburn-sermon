<?xml version="1.0" encoding="UTF-8" ?>
<Configuration>

	<Appenders>
		<Console name="CONSOLE" target="SYSTEM_OUT">
			<PatternLayout pattern="%-5p %d [%t] %c - %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Logger name="org.springframework" level="warn" />
		
		<Root level="debug">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>

</Configuration>
